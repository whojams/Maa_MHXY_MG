    "option": {
        "创建队伍-副本": {
            "cases": [
                {
                    "name": "不需创建队伍",
                    "pipeline_override": {
                        "识别位置-长安城-副本": {
                            "next": [
                                "打开小地图_百晓仙子"
                            ]
                        }
                    }
                },
                {
                    "name": "需要创建队伍",
                    "pipeline_override": {}
                }
            ]
        },
        "320或3本": {
            "cases": [
                {
                    "name": "3副本加两轮鬼",
                    "pipeline_override": {
                        "50普通-2-副本完成-退出": {
                            "next": [
                                "日常抓鬼-第一轮"
                            ]
                        }
                    }
                },
                {
                    "name": "3副本",
                    "pipeline_override": {}
                }
            ]
        },
        "抓鬼-队长-是否创建组队": {
            "cases": [
                {
                    "name": "不创建队伍",
                    "pipeline_override": {
                        "识别位置-长安城-抓鬼": {
                            "next": [
                                "打开小地图-钟馗"
                            ]
                        }
                    }
                },
                {
                    "name": "创建队伍",
                    "pipeline_override": {
                        "识别位置-长安城-抓鬼": {}
                    }
                }
            ]
        },
        "抓鬼进入轮次选择": {
            "cases": [
                {
                    "name": "不进入轮次选择",
                    "pipeline_override": {
                        "抓鬼一轮完成": {}
                    }
                },
                {
                    "name": "进入轮次选择",
                    "pipeline_override": {
                        "抓鬼一轮完成": {
                            "next": [
                                "抓鬼轮次计算-agent"
                            ]
                        }
                    }
                }
            ]
        },
        "是否选择题库": {
            "cases": [
                {
                    "name": "不选择题库",
                    "pipeline_override": {
                        "活动-三界奇缘-进入答题界面": {
                            "interrupt": [
                                "活动-三界奇缘-开始答题"
                            ]
                        }
                    }
                },
                {
                    "name": "选择题库",
                    "pipeline_override": {
                        "活动-三界奇缘-进入答题界面": {
                            "interrupt": [
                                "活动-三界奇缘-开始答题_agent"
                            ]
                        }
                    }
                }
            ]
        },
        "选择宝图时间": {
            "cases": [
                {
                    "name": "69级(11分钟)",
                    "pipeline_override": {
                        "baotu_renwu_sleep": {
                            "pre_delay": 700000
                        }
                    }
                },
                {
                    "name": "25级小号(21分钟)",
                    "pipeline_override": {
                        "baotu_renwu_sleep": {
                            "pre_delay": 1260000
                        }
                    }
                }
            ]
        },
        "全自动/手动接取双侠士任务选项": {
            "default_case": "手动接取",
            "cases": [
                {
                    "name": "全自动",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": []
                        }
                    }
                },
                {
                    "name": "手动接取",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": [
                                "115返回长安城"
                            ]
                        }
                    }
                },
                {
                    "name": "只打普通三本及20鬼",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": [
                                "115打开小地图-百晓仙子-50普通-1"
                            ]
                        }
                    }
                }
            ]
        },
        "是否检测活力到50": {
            "cases": [
                {
                    "name": "不检测（建议选择）",
                    "pipeline_override": {
                        "活动-运镖": {
                            "next": [
                                "活动-运镖-点击日常活动"
                            ]
                        }
                    }
                },
                {
                    "name": "检测",
                    "pipeline_override": {
                        "活动-运镖": {
                            "next": []
                        }
                    }
                }
            ]
        }
    },
    "advanced": {
        "输入文本": {
            "field": "text",
            "type": "string",
            "default": "nihao",
            "pipeline_override": {
                "shuru": {
                    "input_text": "{text}"
                }
            }
        },
        "抓鬼轮次选择": {
            "field": "number",
            "type": "int",
            "default": 2,
            "pipeline_override": {
                "抓鬼轮数": {
                    "custom_action_param": {
                        "target_count": "{number}"
                    }
                }
            }
        }
    }
}