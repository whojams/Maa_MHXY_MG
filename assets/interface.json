    "option": {
        "创建（副本/捉鬼）任务": {
            "type": "select",
            "cases": [
                {
                    "name": "创建副本任务",
                    "pipeline_override": {
                        "队伍-点击日常任务": {
                            "next": [
                                "队伍-点击副本任务"
                            ]
                        }
                    }
                },
                {
                    "name": "创建捉鬼任务",
                    "pipeline_override": {
                        "队伍-点击日常任务": {
                            "next": [
                                "队伍-点击捉鬼任务"
                            ]
                        }
                    }
                }
            ]
        },
        "任务描述(用户输入)": {
            "type": "input",
            "inputs": [
                {
                    "name": "任务描述",
                    "pipeline_type": "string",
                    "default": ""
                }
            ],
            "pipeline_override": {
                "队伍-任务描述-用户输入": {
                    "input_text": "{任务描述}"
                }
            }
        },
        "320或3本": {
            "type": "select",
            "cases": [
                {
                    "name": "3副本加两轮鬼",
                    "pipeline_override": {}
                },
                {
                    "name": "3副本",
                    "pipeline_override": {
                        "50普通-2-副本完成-退出": {
                            "next": [
                                "退出队伍"
                            ]
                        }
                    }
                },
                {
                    "name": "2副本加两轮鬼",
                    "pipeline_override": {
                        "识别位置-长安城-副本-过渡": {
                            "next": [
                                "打开小地图_百晓仙子-50普通-1",
                                "[JumpBack]打开大地图_69副本"
                            ]
                        }
                    }
                }
            ]
        },
        "（开启/关闭）人员检测-（是否）进入轮次选择": {
            "type": "select",
            "cases": [
                {
                    "name": "开启人员检测-不进入轮次选择",
                    "pipeline_override": {
                        "抓鬼-关闭组队界面": {}
                    }
                },
                {
                    "name": "开启人员检测-进入轮次选择",
                    "pipeline_override": {
                        "抓鬼-关闭组队界面": {
                            "next": [
                                "抓鬼轮次计算-agent"
                            ]
                        }
                    }
                },
                {
                    "name": "关闭人员检测-进入轮次选择",
                    "pipeline_override": {
                        "抓鬼一轮完成": {
                            "next": [
                                "[JumpBack]抓鬼一轮完成-再次点击确定",
                                "抓鬼轮次计算-agent"
                            ]
                        }
                    }
                },
                {
                    "name": "关闭人员检测-不进入轮次选择",
                    "pipeline_override": {
                        "抓鬼一轮完成": {
                            "next": [
                                "钟馗-捉鬼任务-循环",
                                "[JumpBack]抓鬼一轮完成-再次点击确定"
                            ]
                        }
                    }
                }
            ]
        },
        "是否选择题库": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "活动-三界奇缘-进入答题界面": {
                            "next": [
                                "活动-三界奇缘-结束",
                                "主界面",
                                "[JumpBack]活动-三界奇缘-开始答题",
                                "[JumpBack]panduan_zhujiemian"
                            ]
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "活动-三界奇缘-进入答题界面": {
                            "next": [
                                "活动-三界奇缘-结束",
                                "主界面",
                                "[JumpBack]活动-三界奇缘-开始答题_agent",
                                "[JumpBack]panduan_zhujiemian"
                            ]
                        }
                    }
                }
            ]
        },
        "选择宝图时间": {
            "type": "select",
            "cases": [
                {
                    "name": "69级(11分钟)",
                    "pipeline_override": {
                        "baotu_renwu_sleep": {
                            "pre_delay": 700000
                        }
                    }
                },
                {
                    "name": "25级小号(21分钟)",
                    "pipeline_override": {
                        "baotu_renwu_sleep": {
                            "pre_delay": 1260000
                        }
                    }
                }
            ]
        },
        "全自动/手动接取双侠士任务选项": {
            "type": "select",
            "default_case": "手动接取",
            "cases": [
                {
                    "name": "全自动",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": []
                        }
                    }
                },
                {
                    "name": "手动接取",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": [
                                "115返回长安城"
                            ]
                        }
                    }
                },
                {
                    "name": "只打普通三本及20鬼",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": [
                                "115打开小地图-百晓仙子-50普通-1"
                            ]
                        }
                    }
                }
            ]
        },
        "是否检测活力到50": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "description": "建议选择不检测",
                    "pipeline_override": {
                        "活动-运镖": {
                            "next": [
                                "活动-运镖-点击日常活动"
                            ]
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "活动-运镖": {
                            "next": []
                        }
                    }
                }
            ]
        },
        "是否使用Ai进行答题": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "活动-科举乡试-进入答题界面": {
                            "next": [
                                "活动-科举乡试-结束-图片",
                                "[JumpBack]活动-科举乡试-开始答题_普通"
                            ]
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "活动-科举乡试-进入答题界面": {
                            "next": [
                                "活动-科举乡试-结束-图片",
                                "[JumpBack]活动-科举乡试-开始答题agent"
                            ]
                        }
                    }
                }
            ]
        },
        "抓鬼轮次输入": {
            "type": "input",
            "inputs": [
                {
                    "name": "抓鬼轮数",
                    "pipeline_type": "int",
                    "default": "2",
                    "verify": "^(0|[1-9]\\d*)$"
                }
            ],
            "pipeline_override": {
                "抓鬼轮数": {
                    "custom_action_param": {
                        "target_count": "{抓鬼轮数}"
                    }
                }
            }
        },
        "Deepseek_apikey": {
            "type": "input",
            "inputs": [
                {
                    "name": "Deepseek_apikey",
                    "pipeline_type": "string",
                    "default": ""
                }
            ],
            "pipeline_override": {
                "AIapikey": {
                    "custom_recognition_param": {
                        "apikey": "{Deepseek_apikey}"
                    }
                }
            }
        },
        "Deepseek_apikey2": {
            "type": "input",
            "inputs": [
                {
                    "name": "Deepseek_apikey",
                    "pipeline_type": "string",
                    "default": ""
                }
            ],
            "pipeline_override": {
                "活动-科举乡试-开始答题agent2": {
                    "attach": {
                        "apikey": "{Deepseek_apikey}"
                    }
                }
            }
        },
        "是否使用自选宠物祈福": {
            "type": "switch",
            "cases": [
                {
                    "name": "否",
                    "pipeline_override": {}
                },
                {
                    "name": "是",
                    "pipeline_override": {
                        "进入选择召唤零界面": {
                            "next": [
                                "用户选择需要祈福的召唤灵",
                                "[JumpBack]祈福-选择宠物-向上滑动"
                            ]
                        }
                    },
                    "option": [
                        "祈福宠物名称"
                    ]
                }
            ]
        },
        "祈福宠物名称": {
            "type": "input",
            "inputs": [
                {
                    "name": "祈福宠物名称",
                    "pipeline_type": "string",
                    "default": ""
                }
            ],
            "pipeline_override": {
                "用户选择需要祈福的召唤灵": {
                    "expected": [
                        "{祈福宠物名称}"
                    ]
                }
            }
        }
    }
}