    "advanced": {
        "输入文本": {
            "field": "text",
            "type": "string",
            "default": "nihao",
            "pipeline_override": {
                "shuru": {
                    "input_text": "{text}"
                }
            }
        },
        "抓鬼轮次选择": {
            "field": "number",
            "type": "int",
            "default": 2,
            "pipeline_override": {
                "抓鬼轮数": {
                    "custom_action_param": {
                        "target_count": "{number}"
                    }
                }
            }
        },
        "Deepseek_apikey": {
            "field": "apikey",
            "type": "string",
            "default": "",
            "pipeline_override": {
                "AIapikey": {
                    "custom_recognition_param": {
                        "apikey": "{apikey}"
                    }
                }
            }
        },
        "Deepseek_apikey2": {
            "field": "apikey",
            "type": "string",
            "default": "",
            "pipeline_override": {
                "活动-科举乡试-开始答题agent2": {
                    "attach": {
                        "apikey": "{apikey}"
                    }
                }
            }
        }
    }
