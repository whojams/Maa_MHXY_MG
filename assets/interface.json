    "option": {
        "创建（副本/捉鬼）任务": {
            "cases": [
                {
                    "name": "创建副本任务",
                    "pipeline_override": {
                        "队伍-点击日常任务": {
                            "next": [
                                "队伍-点击副本任务"
                            ]
                        }
                    }
                },
                {
                    "name": "创建捉鬼任务",
                    "pipeline_override": {
                        "队伍-点击日常任务": {
                            "next": [
                                "队伍-点击捉鬼任务"
                            ]
                        }
                    }
                }
            ]
        },
        "副本任务描述（520/320/捉鬼）": {
            "cases": [
                {
                    "name": "520",
                    "pipeline_override": {
                        "队伍-任务描述": {
                            "next": [
                                "队伍-任务描述-输入520"
                            ]
                        }
                    }
                },
                {
                    "name": "320",
                    "pipeline_override": {
                        "队伍-任务描述": {
                            "next": [
                                "队伍-任务描述-输入320"
                            ]
                        }
                    }
                },
                {
                    "name": "捉鬼",
                    "pipeline_override": {
                        "队伍-任务描述": {
                            "next": [
                                "队伍-任务描述-捉鬼"
                            ]
                        }
                    }
                }
            ]
        },
        "320或3本": {
            "cases": [
                {
                    "name": "3副本加两轮鬼",
                    "pipeline_override": {
                        "50普通-2-副本完成-退出": {
                            "next": [
                                "日常抓鬼-第一轮"
                            ]
                        }
                    }
                },
                {
                    "name": "3副本",
                    "pipeline_override": {}
                }
            ]
        },
        "（开启/关闭）人员检测-（是否）进入轮次选择": {
            "cases": [
                {
                    "name": "开启人员检测-不进入轮次选择",
                    "pipeline_override": {
                        "抓鬼-关闭组队界面": {}
                    }
                },
                {
                    "name": "开启人员检测-进入轮次选择",
                    "pipeline_override": {
                        "抓鬼-关闭组队界面": {
                            "next": [
                                "抓鬼轮次计算-agent"
                            ]
                        }
                    }
                },
                {
                    "name": "关闭人员检测-进入轮次选择",
                    "pipeline_override": {
                        "抓鬼一轮完成": {
                            "next": [
                                "抓鬼轮次计算-agent"
                            ]
                        }
                    }
                },
                {
                    "name": "关闭人员检测-不进入轮次选择",
                    "pipeline_override": {
                        "抓鬼一轮完成": {
                            "next": [
                                "钟馗-捉鬼任务-循环"
                            ]
                        }
                    }
                }
            ]
        },
        "是否选择题库": {
            "cases": [
                {
                    "name": "不选择题库",
                    "pipeline_override": {
                        "活动-三界奇缘-进入答题界面": {
                            "next": [
                                "活动-三界奇缘-结束",
                                "主界面",
                                "[JumpBack]活动-三界奇缘-开始答题",
                                "[JumpBack]panduan_zhujiemian"
                            ]
                        }
                    }
                },
                {
                    "name": "选择题库",
                    "pipeline_override": {
                        "活动-三界奇缘-进入答题界面": {
                            "next": [
                                "活动-三界奇缘-结束",
                                "主界面",
                                "[JumpBack]活动-三界奇缘-开始答题_agent",
                                "[JumpBack]panduan_zhujiemian"
                            ]
                        }
                    }
                }
            ]
        },
        "选择宝图时间": {
            "cases": [
                {
                    "name": "69级(11分钟)",
                    "pipeline_override": {
                        "baotu_renwu_sleep": {
                            "pre_delay": 700000
                        }
                    }
                },
                {
                    "name": "25级小号(21分钟)",
                    "pipeline_override": {
                        "baotu_renwu_sleep": {
                            "pre_delay": 1260000
                        }
                    }
                }
            ]
        },
        "全自动/手动接取双侠士任务选项": {
            "default_case": "手动接取",
            "cases": [
                {
                    "name": "全自动",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": []
                        }
                    }
                },
                {
                    "name": "手动接取",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": [
                                "115返回长安城"
                            ]
                        }
                    }
                },
                {
                    "name": "只打普通三本及20鬼",
                    "pipeline_override": {
                        "115识别位置-长安城-副本": {
                            "next": [
                                "115打开小地图-百晓仙子-50普通-1"
                            ]
                        }
                    }
                }
            ]
        },
        "是否检测活力到50": {
            "cases": [
                {
                    "name": "不检测（建议选择）",
                    "pipeline_override": {
                        "活动-运镖": {
                            "next": [
                                "活动-运镖-点击日常活动"
                            ]
                        }
                    }
                },
                {
                    "name": "检测",
                    "pipeline_override": {
                        "活动-运镖": {
                            "next": []
                        }
                    }
                }
            ]
        },
        "是否使用Ai进行答题": {
            "cases": [
                {
                    "name": "不使用ai",
                    "pipeline_override": {
                        "活动-科举乡试-进入答题界面": {
                            "next": [
                                "活动-科举乡试-结束-图片",
                                "[JumpBack]活动-科举乡试-开始答题_普通"
                            ]
                        }
                    }
                },
                {
                    "name": "使用ai",
                    "pipeline_override": {
                        "活动-科举乡试-进入答题界面": {
                            "next": [
                                "活动-科举乡试-结束-图片",
                                "[JumpBack]活动-科举乡试-开始答题agent"
                            ]
                        }
                    }
                }
            ]
        }
